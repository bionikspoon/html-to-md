{
  "name": "@bionikspoon/html-to-md",
  "description": "A simple CLI for converting html to markdown",
  "version": "0.0.0",
  "author": "Manu Phatak @bionikspoon",
  "bin": {
    "html-to-md": "./bin/run"
  },
  "bugs": "https://github.com/bionikspoon/html-to-md/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "tslib": "2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "8.0.4",
    "@types/node": "14.14.9",
    "chai": "^4",
    "eslint": "7.14",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "0.2",
    "eslint-config-prettier": "6.15.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.2",
    "mocha": "8",
    "nyc": "15",
    "prettier": "2.2.0",
    "ts-node": "9",
    "typescript": "4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/bionikspoon/html-to-md",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "html-to-md"
  },
  "repository": "bionikspoon/html-to-md",
  "scripts": {
    "lint": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only ",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
