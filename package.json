{
  "name": "@bionikspoon/html-to-md",
  "description": "A simple CLI for converting html to markdown",
  "version": "0.0.1-0",
  "author": "Manu Phatak @manuphatak",
  "bin": {
    "html-to-md": "./bin/run"
  },
  "bugs": "https://github.com/bionikspoon/html-to-md/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "1.3.4",
    "@oclif/plugin-autocomplete": "0.2.0",
    "@oclif/plugin-help": "3.2.0",
    "@oclif/plugin-not-found": "1.2.4",
    "cli-ux": "5.5.1",
    "tslib": "2",
    "turndown": "7.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "8.0.4",
    "@types/node": "14.14.9",
    "@types/turndown": "5.0.0",
    "chai": "^4",
    "coveralls": "3.1.0",
    "eslint": "7.14",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "0.2",
    "eslint-config-prettier": "6.15.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.2",
    "mocha": "8",
    "mocha-snapshots": "4.2.0",
    "nyc": "15",
    "prettier": "2.2.0",
    "snap-shot-it": "7.9.3",
    "ts-node": "9",
    "typescript": "4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/bionikspoon/html-to-md",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "html-to-md",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete"
    ]
  },
  "repository": "bionikspoon/html-to-md",
  "scripts": {
    "lint": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "test": "COLUMNS=80 nyc --extension .ts mocha --forbid-only",
    "test:update": "SNAPSHOT_UPDATE=1 yarn test",
    "ci:coverage": "nyc report --reporter=text-lcov | coveralls",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "config": {
    "snap-shot-it": {
      "useRelativePath": true
    }
  }
}
